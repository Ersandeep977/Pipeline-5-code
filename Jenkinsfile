/* This Pipeline code are create the some multi branch pipeline
 This file are content the master branch data on  */

pipeline
{
    agent any
    stages
    {
        stage('Job-1-Master')
        {
            steps
            {
             echo "hello Job-1"    
            }
        }
        stage('Job-2-Master')
        {
            steps
            {
             echo "hello Job-2"    
            }
        }
     stage('Job-3-Master')
        {
            steps
            {
             echo "hello Job-3"    
            }
        } 
    }
}
